#!/bin/sh

set -eu

###################
# Skip Slow Shims #
###################

prog="$0"
cd "$(dirname "$(dirname "$prog")")" || exit 1


if [ -x /usr/local/bin/python3 ]
then
  exec /usr/local/bin/python3  -m iso_cp -- "$prog" "$@"
elif [ -x /usr/bin/python3 ]
then
  exec /usr/bin/python3        -m iso_cp -- "$prog" "$@"
else
  exec /usr/bin/env -S python3 -m iso_cp -- "$prog" "$@"
fi

